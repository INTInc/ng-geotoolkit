{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "../dist/out-tsc",
    "paths": {
      "@int/ng-geotoolkit/common": ["../../projects/common/src/public-api"],
      "@int/ng-geotoolkit/welllog": ["../../projects/welllog/src/public-api"],
      "@int/ng-geotoolkit/gauges": ["../../projects/gauges/src/public-api"],
